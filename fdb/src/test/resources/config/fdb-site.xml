<configuration>
  <property>
    <name>fdb.zookeeper.connection.string</name>
    <value>localhost:2181</value>
  </property>
  <property>
    <name>fdb.zookeeper.path</name>
    <value>/fdb</value>
  </property>
  <property>
    <name>fdb.zookeeper.retry.count</name>
    <value>5</value>
  </property>
  <property>
    <name>fdb.zookeeper.retry.delay.ms</name>
    <value>3000</value>
  </property>
  <property>
    <name>fdb.security.authentication</name>
    <value>NONE</value>
  </property>
  <property>
    <name>fdb.security.spn</name>
    <value>fdb</value>
  </property>
  <property>
    <name>fdb.security.login</name>
    <value>fdb-client</value>
  </property>
  <property>
    <name>fdb.security.keytab</name>
    <value>src/test/resources/config/fdb-client.keytab</value>
  </property>
  <property>
