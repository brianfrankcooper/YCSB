    <value>5000</value>
  </property>
  <property>
    <name>fdb.create.namespace.timeout.ms</name>
    <value>600000</value>
  </property>
  <property>
    <name>fdb.create.table.timeout.ms</name>
    <value>600000</value>
  </property>
  <property>
    <name>fdb.disable.table.timeout.ms</name>
    <value>600000</value>
  </property>
  <property>
    <name>fdb.drop.table.timeout.ms</name>
    <value>600000</value>
  </property>
  <property>
    <name>fdb.security.token.retry.timeout.ms</name>
    <value>1000</value>
  </property>
  <property>
    <name>fdb.security.token.retry.count</name>
    <value>5</value>
  </property>
